local function makeDraggable(frame)
    local dragging=false
    local dragInput=nil
    local dragStart=nil
    local startPos=nil
    frame.InputBegan:Connect(function(input)
        if input.UserInputType==Enum.UserInputType.MouseButton1 then
            dragging=true
            dragStart=input.Position
            startPos=frame.Position
            input.Changed:Connect(function()
                if input.UserInputState==Enum.UserInputState.End then
                    dragging=false
                end
            end)
        end
    end)
    frame.InputChanged:Connect(function(input)
        if input.UserInputType==Enum.UserInputType.MouseMovement then
            dragInput=input
        end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if input==dragInput and dragging then
            local delta=input.Position-dragStart
            frame.Position=UDim2.new(startPos.X.Scale, startPos.X.Offset+delta.X, startPos.Y.Scale, startPos.Y.Offset+delta.Y)
        end
    end)
end
local function createKeyGui()
    local ScreenGui=Instance.new("ScreenGui",player:WaitForChild("PlayerGui"))
    ScreenGui.Name="KeyGui"
    local frame=Instance.new("Frame",ScreenGui)
    frame.Size=UDim2.new(0,300,0,150)
    frame.Position=UDim2.new(0.5,-150,0.5,-75)
    keyFrame.Active = true
    keyFrame.Draggable = true
    keyFrame.Selectable = true
    frame.BackgroundColor3=Color3.fromRGB(30,30,30)
    frame.Active=true
    frame.Draggable=true
    local textBox=Instance.new("TextBox",frame)
    textBox.Size=UDim2.new(0,280,0,40)
    textBox.Position=UDim2.new(0,10,0,40)
    textBox.PlaceholderText="Enter Key"
    textBox.ClearTextOnFocus=false
    textBox.Text=""
    textBox.TextColor3=Color3.new(1,1,1)
    textBox.BackgroundColor3=Color3.fromRGB(20,20,20)
    textBox.Font=Enum.Font.SourceSans
    textBox.TextSize=20
    local submitBtn=Instance.new("TextButton",frame)
    submitBtn.Size=UDim2.new(0,280,0,40)
    submitBtn.Position=UDim2.new(0,10,0,90)
    submitBtn.BackgroundColor3=Color3.fromRGB(50,50,50)
    submitBtn.Text="Submit"
    submitBtn.Font=Enum.Font.SourceSansBold
    submitBtn.TextSize=22
    submitBtn.TextColor3=Color3.new(1,1,1)
    submitBtn.MouseButton1Click:Connect(function()
        if textBox.Text:lower()==key:lower() then
            ScreenGui:Destroy()
            createMainGui()
            guiLoaded=true
        else
            textBox.Text=""
            textBox.PlaceholderText="Incorrect Key"
        end
    end)
end
local function createMainGui()
    local ScreenGui=Instance.new("ScreenGui",player:WaitForChild("PlayerGui"))
    ScreenGui.Name="MainGui"
    local mainFrame=Instance.new("Frame",ScreenGui)
    mainFrame.Name="MainFrame"
    mainFrame.Size=UDim2.new(0,400,0,350)
    mainFrame.Position=UDim2.new(0.5,-200,0.5,-175)
    mainFrame.Active = true
    mainFrame.Draggable = true
    mainFrame.Selectable = true
    mainFrame.BackgroundColor3=Color3.fromRGB(30,30,30)
    mainFrame.Active=true
    makeDraggable(mainFrame)
    local titleBar=Instance.new("Frame",mainFrame)
    titleBar.Size=UDim2.new(1,0,0,30)
    titleBar.BackgroundColor3=Color3.fromRGB(20,20,20)
    local titleLabel=Instance.new("TextLabel",titleBar)
    titleLabel.Text="Heroes: Online World - Cheat GUI"
    titleLabel.Font=Enum.Font.SourceSansBold
    titleLabel.TextSize=18
    titleLabel.TextColor3=Color3.new(1,1,1)
    titleLabel.BackgroundTransparency=1
    titleLabel.Size=UDim2.new(1,-60,1,0)
    titleLabel.TextXAlignment=Enum.TextXAlignment.Left
    titleLabel.Position=UDim2.new(0,10,0,0)
    local closeButton=Instance.new("TextButton",titleBar)
    closeButton.Text="X"
    closeButton.Font=Enum.Font.SourceSansBold
    closeButton.TextSize=18
    closeButton.TextColor3=Color3.new(1,0,0)
    closeButton.BackgroundColor3=Color3.fromRGB(20,20,20)
    closeButton.Size=UDim2.new(0,30,1,0)
    closeButton.Position=UDim2.new(1,-30,0,0)
    closeButton.MouseButton1Click:Connect(function()
        ScreenGui:Destroy()
    end)
    local minimizeButton=Instance.new("TextButton",titleBar)
    minimizeButton.Text="_"
    minimizeButton.Font=Enum.Font.SourceSansBold
    minimizeButton.TextSize=18
    minimizeButton.TextColor3=Color3.new(1,1,1)
    minimizeButton.BackgroundColor3=Color3.fromRGB(20,20,20)
    minimizeButton.Size=UDim2.new(0,30,1,0)
    minimizeButton.Position=UDim2.new(1,-60,0,0)
    local contentFrame=Instance.new("Frame",mainFrame)
    contentFrame.Name="ContentFrame"
    contentFrame.Size=UDim2.new(1,0,1,-30)
    contentFrame.Position=UDim2.new(0,0,0,30)
    contentFrame.BackgroundColor3=Color3.fromRGB(40,40,40)
    minimizeButton.MouseButton1Click:Connect(function()
        if contentFrame.Visible==true then
            contentFrame.Visible=false
            minimizeButton.Text="Open"
        else
            contentFrame.Visible=true
            minimizeButton.Text="_"
        end
    end)
    local invincibleBtn=Instance.new("TextButton",contentFrame)
    invincibleBtn.Size=UDim2.new(0,180,0,50)
    invincibleBtn.Position=UDim2.new(0,10,0,10)
    invincibleBtn.BackgroundColor3=Color3.fromRGB(70,70,70)
    invincibleBtn.Text="Invincibility OFF"
    invincibleBtn.Font=Enum.Font.SourceSansBold
    invincibleBtn.TextSize=18
    invincibleBtn.TextColor3=Color3.new(1,1,1)
    local invincibleEnabled=false
    local function setInvincible(state)
        invincibleEnabled=state
        if state then
            invincibleBtn.Text="Invincibility ON"
            local character=player.Character
            if character then
                local humanoid=character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid.MaxHealth=math.huge
                    humanoid.Health=math.huge
                    humanoid:GetPropertyChangedSignal("Health"):Connect(function()
                        if invincibleEnabled and humanoid.Health<humanoid.MaxHealth then
                            humanoid.Health=humanoid.MaxHealth
                        end
                    end)
                end
            end
        else
            invincibleBtn.Text="Invincibility OFF"
            local character=player.Character
            if character then
                local humanoid=character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid.MaxHealth=100
                    if humanoid.Health>100 then humanoid.Health=100 end
                end
            end
        end
    end
    invincibleBtn.MouseButton1Click:Connect(function()
        setInvincible(not invincibleEnabled)
    end)
    local coinsBtn=Instance.new("TextButton",contentFrame)
    coinsBtn.Size=UDim2.new(0,180,0,50)
    coinsBtn.Position=UDim2.new(0,10,0,70)
    coinsBtn.BackgroundColor3=Color3.fromRGB(70,70,70)
    coinsBtn.Text="Infinite Coins"
    coinsBtn.Font=Enum.Font.SourceSansBold
    coinsBtn.TextSize=18
    coinsBtn.TextColor3=Color3.new(1,1,1)
    coinsBtn.MouseButton1Click:Connect(function()
        local leaderstats=player:FindFirstChild("leaderstats")
        if leaderstats then
            local coins=leaderstats:FindFirstChild("Coins") or leaderstats:FindFirstChild("Coin")
            if coins then
                coins.Value=1e9
            end
        end
    end)
    local unlockBtn=Instance.new("TextButton",contentFrame)
    unlockBtn.Size=UDim2.new(0,180,0,50)
    unlockBtn.Position=UDim2.new(0,10,0,130)
    unlockBtn.BackgroundColor3=Color3.fromRGB(70,70,70)
    unlockBtn.Text="Unlock All Characters"
    unlockBtn.Font=Enum.Font.SourceSansBold
    unlockBtn.TextSize=18
    unlockBtn.TextColor3=Color3.new(1,1,1)
    unlockBtn.MouseButton1Click:Connect(function()
        local charFolder=player:FindFirstChild("Characters") or player:FindFirstChild("OwnedCharacters")
        if not charFolder then
            charFolder=Instance.new("Folder",player)
            charFolder.Name="Characters"
        end
        for _,charName in ipairs(characters) do
            if not charFolder:FindFirstChild(charName) then
                local c=Instance.new("BoolValue",charFolder)
                c.Name=charName
                c.Value=true
            end
        end
    end)
end
createKeyGui()
